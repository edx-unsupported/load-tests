          <stringProp name="parameters"></stringProp>
          <stringProp name="script">//vars.put(&quot;SWITCH_VALUE&quot;,&quot;1&quot;);

java.util.Random random = new java.util.Random();

int iAlea = random.nextInt(100);
if (0 &lt;= iAlea &amp;&amp; iAlea &lt; 10) {
// 10%
sCase = &quot;0&quot;;
}
if (10 &lt;= iAlea &amp;&amp; iAlea &lt; 30) {
// 20%
sCase = &quot;1&quot;;
}
if (30 &lt;= iAlea &amp;&amp; iAlea &lt; 60) {
// 30%
sCase = &quot;2&quot;;
}
if (60 &lt;= iAlea &amp;&amp; iAlea &lt; 100) {
// 40%
sCase = &quot;3&quot;;
}
log.info(&quot;sCase = &quot; + sCase);
vars.put(&quot;SWITCH_VALUE&quot;,sCase);</stringProp>
